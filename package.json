{
  "name": "react-boiterplate",
  "version": "0.0.2",
  "description": "Starter boiterplate for webapp with React.",
  "main": "src/index.js",
  "repository": "git@github.com:ngtrhieu/react-boiterplate.git",
  "author": "ngtrhieu",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "start": "cross-env NODE_ENV=development webpack-dev-server --open",
    "build": "yarn clean && cross-env NODE_ENV=development webpack",
    "build:prod": "yarn clean && cross-env NODE_ENV=production webpack",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "lint:staged": "lint-staged",
    "commit": "git-cz",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint:staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn lint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-brands-svg-icons": "^5.13.1",
    "@fortawesome/free-regular-svg-icons": "^5.13.1",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.19.2",
    "cross-env": "^7.0.2",
    "eslint": "^7.2.0",
    "lodash": "^4.17.19",
    "lodash-fp": "^0.10.4",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sanitize.css": "^11.0.1",
    "typeface-roboto": "^0.0.75",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "circular-dependency-plugin": "^5.2.0",
    "commitizen": "^4.1.2",
    "compression-webpack-plugin": "^4.0.0",
    "css-loader": "^3.6.0",
    "cz-conventional-changelog": "3.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-lodash-fp": "^2.2.0-a1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "image-webpack-loader": "^6.0.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "react-app-polyfill": "^1.0.6",
    "rimraf": "^3.0.2",
    "standard-version": "^8.0.0",
    "style-loader": "^1.2.1",
    "svg-url-loader": "^6.0.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
